{
	"version": "2",
	"templates": [
		{
			"title": "Homer",
			"description": "A dead simple static HOMepage for your servER to keep your services on hand, from a simple yaml configuration file.",
			"logo": "https://raw.githubusercontent.com/univrs-cloud/portainer/main/images/homer.png",
			"categories": [
				"Other",
				"Tools"
			],
			"image": "b4bz/homer:latest",
			"name": "homer",
			"platform": "linux",
			"ports": [
				"80:8080/tcp"
			],
			"restart_policy": "unless-stopped",
			"type": 1,
			"volumes": [
				{
					"bind": "/portainer/Files/AppData/Config/Homer/assets",
					"container": "/www/assets"
				}
			]
		},
		{
			"title": "Pi-Hole + Unbound",
			"description": "A Linux network-level advertisement and Internet tracker blocking application which acts as a DNS sinkhole. This version has Ubound software installed on it so you don't need to rely on external DNS providers.",
			"logo": "https://raw.githubusercontent.com/univrs-cloud/portainer/main/images/pihole.png",
			"categories": [
				"Other",
				"Tools"
			],
			"env": [
				{
					"default": "monolith",
					"label": "VIRTUAL_HOST",
					"name": "VIRTUAL_HOST"
				},
				{
					"default": "univrs",
					"label": "DOMAIN_NAME",
					"name": "DOMAIN_NAME"
				},
				{
					"default": "192.168.0.2",
					"label": "FTLCONF_LOCAL_IPV4",
					"name": "FTLCONF_LOCAL_IPV4"
				},
				{
					"default": "true",
					"label": "DHCP_ACTIVE",
					"name": "DHCP_ACTIVE"
				},
				{
					"default": "192.168.0.100",
					"label": "DHCP_START",
					"name": "DHCP_START"
				},
				{
					"default": "192.168.0.250",
					"label": "DHCP_END",
					"name": "DHCP_END"
				},
				{
					"default": "192.168.0.1",
					"label": "DHCP_ROUTER",
					"name": "DHCP_ROUTER"
				},
				{
					"default": "pa$$word",
					"label": "WEBPASSWORD",
					"name": "WEBPASSWORD"
				},
				{
					"default": "Europe\\London",
					"label": "TZ",
					"name": "TZ"
				}
			],
			"name": "pihole",
			"platform": "linux",
			"repository": {
				"url": "https://github.com/univrs-cloud/portainer/",
				"stackfile": "stack/pihole.yaml"
			},
			"type": 3
		},
		{
			"title": "WireGuard Server",
			"description": "An extremely simple yet fast and modern VPN.",
			"logo": "https://raw.githubusercontent.com/univrs-cloud/portainer/main/images/wireguard.png",
			"categories": [
				"Other",
				"VPN"
			],
			"env": [
				{
					"description": "DDNS domain",
					"default": "example.domain.com",
					"label": "WG_HOST",
					"name": "WG_HOST"
				},
				{
					"description": "Leave blank to access WebUI without loggin",
					"default": "ENTER A PASSWORD",
					"label": "PASSWORD",
					"name": "PASSWORD"
				},
				{
					"default": "51820",
					"label": "WG_PORT",
					"name": "WG_PORT"
				},
				{
					"default": "1.1.1.1",
					"label": "WG_DEFAULT_DNS",
					"name": "WG_DEFAULT_DNS"
				},
				{
					"default": "10.6.0.x",
					"label": "WG_DEFAULT_ADDRESS",
					"name": "WG_DEFAULT_ADDRESS"
				},
				{
					"default": "0.0.0.0/0, ::/0",
					"label": "WG_ALLOWED_IPS",
					"name": "WG_ALLOWED_IPS"
				}
			],
			"name": "wg-easy",
			"platform": "linux",
			"repository": {
				"url": "https://github.com/univrs-cloud/portainer/",
				"stackfile": "stack/wireguard-easy.yaml"
			},
			"type": 3
		},
		{
			"title": "WeTTY",
			"description": "Terminal over HTTP and https.",
			"logo": "https://raw.githubusercontent.com/univrs-cloud/portainer/main/images/terminal.png",
			"categories": [
				"Other",
				"Tools"
			],
			"env": [
				{
					"description": "Host",
					"default": "192.168.0.100",
					"label": "SSHHOST",
					"name": "SSHHOST"
				},
				{
					"description": "User",
					"default": "dave",
					"label": "SSHUSER",
					"name": "SSHUSER"
				}
			],
			"image": "wettyoss/wetty:latest",
			"name": "wetty",
			"platform": "linux",
			"ports": [
				"2222:3000/tcp"
			],
			"restart_policy": "unless-stopped",
			"type": 1
		},
		{
			"title": "qBittorrent",
			"description": "The qBittorrent project aims to provide an open-source software alternative to ÂµTorrent. qBittorrent is based on the Qt toolkit and libtorrent-rasterbar library.",
			"logo": "https://raw.githubusercontent.com/univrs-cloud/portainer/main/images/qbittorrent.png",
			"categories": [
				"Downloaders"
			],
			"env": [
				{
					"default": "1000",
					"label": "PUID",
					"name": "PUID"
				},
				{
					"default": "100",
					"label": "PGID",
					"name": "PGID"
				}
			],
			"image": "lscr.io/linuxserver/qbittorrent:latest",
			"name": "qbittorrent",
			"platform": "linux",
			"ports": [
				"6881:6881/tcp",
				"6881:6881/udp",
				"6080:8080/tcp"
			],
			"restart_policy": "unless-stopped",
			"type": 1,
			"volumes": [
				{
					"bind": "/portainer/Files/AppData/Config/qBittorrent",
					"container": "/config"
				},
				{
					"bind": "/downloads",
					"container": "/downloads"
				}
			]
		}
	]
}
